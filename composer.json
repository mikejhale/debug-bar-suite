{
    "name": "stompgear/debug-bar-suite",
    "description": "Installs Debug Bar and Add-ons in a single plugin.",
    "license": "GPLv2+",
    "authors": [
        {
            "name": "Mike Hale",
            "email": "mike@stompgear.com"
        }
    ],
    "repositories" : [
        {
             "type" : "composer",
             "url"  : "https://wpackagist.org"
        }
    ],
    "require": {
        "wpackagist-plugin/debug-bar": "^0.9.0",
        "wpackagist-plugin/debug-bar-console": "^0.3.0",
        "wpackagist-plugin/debug-bar-shortcodes": "^2.0",
        "wpackagist-plugin/debug-bar-constants": "^2.0",
        "wpackagist-plugin/debug-bar-post-types": "^2.0",
        "wpackagist-plugin/debug-bar-cron": "^0.1.3",
        "wpackagist-plugin/debug-bar-actions-and-filters-addon": "^1.5",
        "wpackagist-plugin/debug-bar-transients": "^0.5.0",
        "wpackagist-plugin/debug-bar-list-dependencies": "^1.1",
        "wpackagist-plugin/debug-bar-remote-requests": "^0.1.2",
        "wpackagist-plugin/query-monitor": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Debug_Bar_Suite\\": ["includes/"]
        },
        "files": [
            "wp-content/plugins/debug-bar/debug-bar.php"
        ]
    }
}
